<h1>user info</h1>

<%= form_with model: @user do |f| %>
 <label for="inputName">Name</label>
 <%= f.text_field :name, autofocus: true, id:"inputName" %><br>
 <label for ="inputImage">ProfileImage</label><br>
 <%= f.file_field :image,placeholder: "プロフィール画像", accept: "image/*" %><br>
 <label for="inputUser_text">Introduction</label>
 <%= f.text_field :introduction, autofocus: true, id:"inputUser_text" %><br>
 <%= f.submit "変更を保存" %>
 <% end %>